{
  "name": "fns-web",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200 --g",
    "build": "webpack",
    "test": "jest --no-cache",
    "lint": "ng lint",
    "freshtall": "npm cache clean --force && rm -rf node_modules && rm -f package-lock.json && npm install",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.6",
    "@angular/cdk": "7.3.1",
    "@angular/common": "7.2.6",
    "@angular/compiler": "7.2.6",
    "@angular/core": "7.2.6",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "7.2.6",
    "@angular/material": "7.3.1",
    "@angular/platform-browser": "7.2.6",
    "@angular/platform-browser-dynamic": "7.2.6",
    "@angular/router": "7.2.6",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "core-js": "2.5.4",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "notosans-fontface": "1.1.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.4.0",
    "tslib": "1.9.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.1",
    "@angular/cli": "7.3.0",
    "@angular/compiler-cli": "7.2.6",
    "@angular/language-service": "7.2.6",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.112",
    "@types/node": "8.9.4",
    "codelyzer": "4.5.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.1.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "testResultsProcessor": "<rootDir>/node_modules/jest-junit-reporter",
    "collectCoverageFrom": [
      "**/src/app/**.{ts}",
      "!**/src/app/**module.{ts}",
      "!**/src/app/**model.{ts}",
      "!**/src/app/layout/**.{ts}",
      "!**/src/app/simplified-layout/**.{ts}",
      "!**/src/app/*.{ts}",
      "!**/src/app/_models/**.{ts}",
      "!**/src/app/shared/_models/**.{ts}",
      "!**/node_modules/**"
    ],
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!ngx-quill)"
    ]
  }
}
